    "# TODO: Edit the function to return the correct grade for different score\n",
    "def get_grade(score):\n",
    "    if score >= 90:\n",
    "        return \"A\"\n",
    "    elif score >= 80:\n",
    "        return \"B\"\n",
    "    elif score >= 70:\n",
    "        return \"C\"\n",
    "    elif score >= 60:\n",
    "        return \"D\"\n",
    "    else:\n",
    "        return \"F\"\n",
    "\n",
    "# Example usage:\n",
    "print(get_grade(85))  # Expected output: \"B\"\n",
    "print(get_grade(49))  # Expected output: \"F\"\n"
   ]
